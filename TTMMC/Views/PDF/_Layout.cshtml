<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/css/pdf.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/pdf.min.css" asp-append-version="true" />
    </environment>
</head>
<body>
    <center>
        @RenderBody()
    </center>
</body>
@RenderSection("Scripts", required: false)
</html>